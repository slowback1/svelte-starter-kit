{
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": false,
    "types": ["cypress", "cypress-axe"],
    "moduleResolution": "bundler",
    "experimentalDecorators": true
  },
}
